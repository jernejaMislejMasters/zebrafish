
#without NDMCHigh, since the dosage is different

#Light
ls *Light | grep -v NDMCHigh | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Light Control ;done
ls *Light | grep -v NDMCHigh | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Light 1microM ;done
ls *Light | grep -v NDMCHigh | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Light 3microM ;done
ls *Light | grep -v NDMCHigh | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Light 10microM ;done

#Dark
ls *Dark | grep -v NDMCHigh | grep -v Light | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Dark Control ;done
ls *Dark | grep -v NDMCHigh | grep -v Light | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Dark 1microM ;done
ls *Dark | grep -v NDMCHigh | grep -v Light | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Dark 3microM ;done
ls *Dark | grep -v NDMCHigh | grep -v Light | while read line;do python3 ../../../../code/transitions_per_time/transitions_per_time_extract_data_B2.py $line Dark 10microM ;done

