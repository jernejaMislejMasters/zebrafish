#combine controls
cd Light
rm Natural_Control_Light_word2
rm Natural_Control_Light_word3
Rscript ../../../../code/transitions_per_time/merge_control_B.R Natural Light
cd ..

cd Dark
rm Natural_Control_Dark_word2
rm Natural_Control_Dark_word3
Rscript ../../../../code/transitions_per_time/merge_control_B.R Natural Dark
cd ..

cd LightDark
rm Natural_Control_LightDark_word2
rm Natural_Control_LightDark_word3
Rscript ../../../../code/transitions_per_time/merge_control_B.R Natural LightDark
cd ..

cd DarkApoLow
rm Disease_Control_DarkApoLow_word2
rm Disease_Control_DarkApoLow_word3
Rscript ../../../../code/transitions_per_time/merge_control_B.R Disease DarkApoLow
cd ..

cd DarkApoHigh
rm Disease_Control_DarkApoHigh_word2
rm Disease_Control_DarkApoHigh_word3
Rscript ../../../../code/transitions_per_time/merge_control_B.R Disease DarkApoHigh
cd ..

cd DarkPTZ
rm Disease_Control_DarkPTZ_word2
rm Disease_Control_DarkPTZ_word3
Rscript ../../../../code/transitions_per_time/merge_control_B.R Disease DarkPTZ
cd ..

