#run python scripts

#Light
ls *Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Light Control ;done
ls *Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Light 3microM ;done
ls *Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Light 1microM ;done
ls *Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Light 10microM ;done

# in all 6  do this, as these are empty
cd Light
rm *NDMCHigh_3microM*
rm *NDMCHigh_1microM*
rm *NDMCHigh_10microM*
cd ..

ls NDMCH*Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Light 100microM ;done
ls NDMCH*Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Light 50microM ;done
ls NDMCH*Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Light 25microM ;done


#Dark
ls *Dark | grep -v Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Dark Control ;done
ls *Dark | grep -v Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Dark 3microM ;done
ls *Dark | grep -v Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Dark 1microM ;done
ls *Dark | grep -v Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Dark 10microM ;done

# in all 6  do this, as these are empty
cd Dark
rm *NDMCHigh_3microM*
rm *NDMCHigh_1microM*
rm *NDMCHigh_10microM*
cd ..

ls NDMCH*Dark | grep -v Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Dark 100microM ;done
ls NDMCH*Dark | grep -v Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Dark 50microM ;done
ls NDMCH*Dark | grep -v Light | while read line;do python3 ../../../../code/simple_motif_search/simple_motif_search.py $line Dark 25microM ;done


