#COMBINE ALL THE CONTROL SUBJECTS FROM DIFFERENT EXPERIMENTS

#Light
cd Light
rm Natural_Control*
cat *Control*motif2 > Natural_Control_Light_motif2
cat *Control*motif3 > Natural_Control_Light_motif3
cat *Control*motif4 > Natural_Control_Light_motif4
cat *Control*motif5 > Natural_Control_Light_motif5
cd ..


#Dark
cd Dark
rm Natural_Control*
cat *Control*motif2 > Natural_Control_Dark_motif2
cat *Control*motif3 > Natural_Control_Dark_motif3
cat *Control*motif4 > Natural_Control_Dark_motif4
cat *Control*motif5 > Natural_Control_Dark_motif5
cd ..


#LightDark
cd LightDark
rm Natural_Control*
cat *Control*motif2 > Natural_Control_LightDark_motif2
cat *Control*motif3 > Natural_Control_LightDark_motif3
cat *Control*motif4 > Natural_Control_LightDark_motif4
cat *Control*motif5 > Natural_Control_LightDark_motif5
cd ..


#DarkApoLow
cd DarkApoLow
rm Disease_Control*
cat *Control*motif2 > Disease_Control_DarkApoLow_motif2
cat *Control*motif3 > Disease_Control_DarkApoLow_motif3
cat *Control*motif4 > Disease_Control_DarkApoLow_motif4
cat *Control*motif5 > Disease_Control_DarkApoLow_motif5
cd ..


#DarkApoHigh
cd DarkApoHigh
rm Disease_Control*
cat *Control*motif2 > Disease_Control_DarkApoHigh_motif2
cat *Control*motif3 > Disease_Control_DarkApoHigh_motif3
cat *Control*motif4 > Disease_Control_DarkApoHigh_motif4
cat *Control*motif5 > Disease_Control_DarkApoHigh_motif5
cd ..


#DarkPTZ
cd DarkPTZ
rm Disease_Control*
cat *Control*motif2 > Disease_Control_DarkPTZ_motif2
cat *Control*motif3 > Disease_Control_DarkPTZ_motif3
cat *Control*motif4 > Disease_Control_DarkPTZ_motif4
cat *Control*motif5 > Disease_Control_DarkPTZ_motif5
cd ..


