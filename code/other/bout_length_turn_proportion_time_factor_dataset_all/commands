
#Natural for Light,Dark,LightDark, Disease for ApoLow, ApoHigh, PTZ



#without NDMCHigh, since the dosage is different

#Light
ls *Light | grep -v NDMCHigh | while read line;do python3 ../../../../code/other/bout_length_turn_proportion_time_factor_dataset_all/generate_boutLength_turnProportion_timeFactor_dataset_all.py $line Light Control ;done
ls *Light | grep -v NDMCHigh | while read line;do python3 ../../../../code/other/bout_length_turn_proportion_time_factor_dataset_all/generate_boutLength_turnProportion_timeFactor_dataset_all.py $line Light 1microM ;done
ls *Light | grep -v NDMCHigh | while read line;do python3 ../../../../code/other/bout_length_turn_proportion_time_factor_dataset_all/generate_boutLength_turnProportion_timeFactor_dataset_all.py $line Light 3microM ;done
