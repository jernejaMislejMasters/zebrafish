#COMBINE ALL THE CONTROL SUBJECTS FROM DIFFERENT EXPERIMENTS

#Light
cd Light
rm All*
cat *_BoutLength > AllLight_BoutLength
cat *_TimeFactor > AllLight_TimeFactor
cat *_ScootsProportion > AllLight_ScootsProportion
cat *_JBendsProportion > AllLight_JBendsProportion
cat *_CBendsProportion > AllLight_CBendsProportion
cat *_OBendsProportion > AllLight_OBendsProportion
cat *_EBendsProportion > AllLight_EBendsProportion
cat *_GBendsProportion > AllLight_GBendsProportion
cat *_HBendsProportion > AllLight_HBendsProportion
cat *_IBendsProportion > AllLight_IBendsProportion
cd ..

#Dark
cd Dark
rm All*
cat *_BoutLength > AllDark_BoutLength
cat *_TimeFactor > AllDark_TimeFactor
cat *_ScootsProportion > AllDark_ScootsProportion
cat *_JBendsProportion > AllDark_JBendsProportion
cat *_CBendsProportion > AllDark_CBendsProportion
cat *_OBendsProportion > AllDark_OBendsProportion
cat *_EBendsProportion > AllDark_EBendsProportion
cat *_GBendsProportion > AllDark_GBendsProportion
cat *_HBendsProportion > AllDark_HBendsProportion
cat *_IBendsProportion > AllDark_IBendsProportion
cd ..

